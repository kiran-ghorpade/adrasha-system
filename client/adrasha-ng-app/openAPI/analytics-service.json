        },
        "security": [
          {
            "BearerAuthentication": []
          }
        ]
      }
    },
    "/analytics/masterdata/stats": {
      "get": {
        "tags": [
          "Analytics Summary"
        ],
        "operationId": "getMasterDataStats",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/FamilyPageResponseDTO"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
