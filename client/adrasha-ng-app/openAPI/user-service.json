  "components": {
    "schemas": {
      "Name": {
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string",
            "maxLength": 100,
            "minLength": 2
          },
          "middlename": {
            "type": "string",
            "maxLength": 100,
            "minLength": 2
          },
          "lastname": {
            "type": "string",
            "maxLength": 100,
            "minLength": 2
          }
        },
        "required": [
          "firstname",
          "lastname"
        ]
      },
      "UserUpdateDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "$ref": "#/components/schemas/Name"
          },
          "healthCenterId": {
            "type": "string",
            "format": "uuid"
          },
          "adharNumber": {
            "type": "string",
            "maxLength": 12,
            "minLength": 12
          }
        },
        "required": [
          "healthCenterId",
          "id"
        ]
      },
      "UserResponseDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "$ref": "#/components/schemas/Name"
          },
          "healthCenterId": {
            "type": "string",
            "format": "uuid"
          },
          "adharNumber": {
            "type": "string"
          },
          "roles": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "SYSTEM",
                "USER",
                "ADMIN",
                "ASHA"
              ]
            },
            "uniqueItems": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "ApiError": {
        "type": "object",
        "properties": {
          "field": {
            "type": "string"
